reset
set output "data_template.ps"
set terminal postscript eps monochrome enhanced dashed
set style data linespoints
set xlabel "base pair"
set format y "" 
set size 1,20
set multiplot

#number of cluster
n=20
do for [i=1:n]{
   set size 1, 1
   set origin 0, (20-i)

   set key top
   plot 'A'.i using 1:2 title "DnaseI level".i with lines lc rgb "red" 
   #set key bottom
   #plot 'cons'.i using 1:2 title "conservation level ".i with lines lc rgb "blue"
}



unset multiplot

